}

.clx-focus-fontColor {
    color: #43af94;
}

.clx-href-fontColor {
    color: #56b39d;
}

.clx-font-bold {
    font-weight: bold;
}

/*字体位置*/
.text-center {
    text-align: center;
}

.text-left {
    text-align: left;
}

.text-right {
    text-align: right;
}

.text-indent {
    text-indent: 2em;
}

/*盒子位置*/
.clx-box-mid {
    margin-left: auto;
    margin-right: auto;
}

/*浮动*/
.clx-fl {

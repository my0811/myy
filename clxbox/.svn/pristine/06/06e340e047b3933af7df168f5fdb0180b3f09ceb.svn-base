@font-face {
    font-family: "思源黑体";
    src: url("../fonts/SourceHanSansCN-Normal.otf");
}

html,
body,
h1,
h2,
h3,
h4,
h5,
h6,
div,
dl,
dt,
dd,
ul,
ol,
li,
p,
blockquote,
pre,
hr,
figure,
table,
caption,
th,
td,
form,
fieldset,
legend,
input,
button,
textarea,
menu {
    font-family: "微软雅黑";
    font-size: 16px;
    font-weight: normal;
}

/*字体共通样式*/
.clx-fontNormal-famliy {
    font-family: "微软雅黑";
}

.clx-SYfont-famliy {
    font-family: "思源黑体";
}

.clx-font-xs {
    font-size: 14px;
}

.clx-font-s {
    font-size: 16px;
}

.clx-font-m {
    font-size: 18px;
}

.clx-font-l {
    font-size: 20px;
}

.clx-font-ml {
    font-size: 24px;
}

.clx-font-xl {
    font-size: 28px;
}

.clx-font-xxl {
    font-size: 42px;
}

/*折叠*/
.clx-fold-down {
    width: 29px;
    height: 18px;
    background: url("../img/clx-banner.png") -290px 0 no-repeat;
    margin: 10px 0 0 800px;
    display: block;
    cursor: pointer;
}

.clx-fold-down:hover {
    background: url("../img/clx-banner.png") -435px 0 no-repeat;
}

.clx-fold-up {
    width: 29px;
    height: 18px;
    background: url("../img/clx-banner.png") -290px -30px no-repeat;
    margin: 10px 0 0 800px;
    display: block;
    cursor: pointer;
}

.clx-fold-up:hover {
    background: url("../img/clx-banner.png") -435px -29px no-repeat;
}

/*字体颜色*/
.clx-btn-fontColor {
    color: white;
}

.clx-article-fontColor {
